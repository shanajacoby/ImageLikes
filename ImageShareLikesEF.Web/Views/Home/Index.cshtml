@using ImageShareLikesEF.Data
@model HomePageViewModel

@foreach (Image image in Model.Images)
{
<div class="row">
    <div class="col-md-8 offset-md-2">
        <div class="jumbotron" style="margin-bottom: 20px; text-align: center;">
            <a href="/home/viewimage?id=@image.Id">
                <h3>@image.Title</h3>
                <h5>@image.DateUploaded</h5>
                <img style="width: 400px;" src="/uploads/@image.ImageName" />
            </a>
        </div>
    </div>
</div>
}